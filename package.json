{
  "name": "widgetfarm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./button": {
      "types": "./dist/button.es.d.ts",
      "import": "./dist/button.es.js",
      "require": "./dist/button.cjs.js"
    },
    "./themes/checkbox/kinda-retro-base.module.css": {
      "import": "./dist/themes/Checkbox/themes/kinda-retro-base.module.css"
    },
    "./themes/button/kinda-retro-light.module.css": {
      "import": "./dist/themes/Button/themes/kinda-retro-light.module.css"
    },
    "./themes/button/kinda-retro-base.module.css": {
      "import": "./dist/themes/Button/themes/kinda-retro-base.module.css"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "bun run build:lib",
    "build:lib": "cross-env BUILD_LIB=1 vite build && bun run copy:themes && bun scripts/generate-exports.js",
    "build:demo": "tsc -b && vite build",
    "copy:themes": "cpy \"src/components/**/themes/*.module.css\" dist/themes --parents"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@ark-ui/react": "^5.29.1",
    "@tanstack/react-router": "^1.139.12",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/router-plugin": "^1.139.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "ajv": "^8.17.1",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}